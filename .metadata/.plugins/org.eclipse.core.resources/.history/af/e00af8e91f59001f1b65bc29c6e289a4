package ex01;

import java.util.Scanner;

/*
 * 나이하고 이름을 키보드 통해서 입력 받아서 
 * 출력
 */
public class InputNameAge {

	public static void main(String[] args) {
		/*
		 * 기본자료형 -> 
		 *  정수형
		 *  	byte, short, int , long
		 *  실수형
		 *  	float, double
		 *  논리형
		 *  	boolean
		 *  문자형
		 *  	char    
		 */
		Scanner sc = new Scanner(System.in);    
		
		System.out.println("이름을 입력하세요 : ");
		
		sc.nextLine();

	}

}
